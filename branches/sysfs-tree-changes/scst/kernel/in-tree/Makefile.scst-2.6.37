ccflags-y += -Wno-unused-parameter

scst-y        += scst_main.o
scst-y        += scst_pres.o
scst-y        += scst_targ.o
scst-y        += scst_lib.o
scst-y        += scst_sysfs.o
scst-y        += scst_mem.o
scst-y        += scst_tg.o
scst-y        += scst_debug.o
scst-$(CONFIG_DEBUG_FS)		+= scst_debugfs.o
scst-$(CONFIG_SCST_DEBUG)	+= scst_tracing.o scst_mem_stats.o
